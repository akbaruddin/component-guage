{
  "name": "components-gauge",
  "license": "MIT",
  "version": "0.0.40",
  "type": "module",
  "main": "./dist/components-gauge.umd.js",
  "module": "./dist/components-gauge.es.js",
  "files": [
    "dist"
  ],
  "source": "src/index.js",
  "scripts": {
    "build": "tsc && vite build && tsup",
    "precommit": "lint-staged",
    "dev": "vite",
    "prepare": "husky install",
    "prepublishOnly": "yarn build",
    "release": "release-it",
    "serve": "vite preview",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "serve-storybook": "serve storybook-static"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@release-it/conventional-changelog": "^5.1.1",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-google-analytics": "^6.2.9",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack4": "^6.5.13",
    "@storybook/manager-webpack4": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-loader": "^9.1.0",
    "husky": "^8.0.2",
    "release-it": "^15.5.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^16.13.1 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.13.1 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "keywords": [
    "react",
    "reactjs",
    "reactjs-component",
    "component",
    "gauge"
  ],
  "description": "Simple React Component to create Gauge.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akbaruddin/components-gauge.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/akbaruddin/components-gauge/issues"
  },
  "homepage": "https://github.com/akbaruddin/components-gauge#readme",
  "volta": {
    "node": "16.17.0",
    "npm": "8.19.1"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8.19.1"
  },
  "exports": {
    ".": {
      "import": "./dist/components-gauge.es.js",
      "require": "./dist/components-gauge.umd.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,tsx,ts}": [
      "prettier --write"
    ]
  },
  "directories": {
    "example": "example"
  }
}
